{
  "_version": "1.60.0",
  "sap.app": {
    "id": "ehsm",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.0",
      "toolsId": "bea41339-b9a3-46a5-b751-15dba56ae427"
    },
    "dataSources": {
      "Z898_SERVICE_BINDING_VAN": {
        "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='Z898_SERVICE_BINDING_VAN',Version='0001')/$value/",
        "type": "ODataAnnotation",
        "settings": {
          "localUri": "localService/mainService/Z898_SERVICE_BINDING_VAN.xml"
        }
      },
      "mainService": {
        "uri": "/sap/opu/odata/sap/Z898_SERVICE_BINDING/",
        "type": "OData",
        "settings": {
          "annotations": [
            "Z898_SERVICE_BINDING_VAN"
          ],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.120.14",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ehsm.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      },
      "session": {
        "type": "sap.ui.model.json.JSONModel"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "ehsm.view",
        "async": true,
        "viewPath": "ehsm.view"
      },
      "routes": [
        {
          "name": "Login",
          "pattern": "",
          "target": [
            "TargetLogin"
          ]
        },
        {
          "name": "Dashboard",
          "pattern": "dashboard",
          "target": [
            "TargetDashboard"
          ]
        },
        {
          "name": "IncidentManagement",
          "pattern": "incident-management",
          "target": [
            "TargetIncidentManagement"
          ]
        },
        {
          "name": "RiskAssessment",
          "pattern": "risk-assessment",
          "target": [
            "TargetRiskAssessment"
          ]
        }
      ],
      "targets": {
        "TargetLogin": {
          "id": "login",
          "name": "Login"
        },
        "TargetDashboard": {
          "id": "dashboard",
          "name": "Dashboard"
        },
        "TargetIncidentManagement": {
          "id": "incidentManagement",
          "name": "IncidentManagement"
        },
        "TargetRiskAssessment": {
          "id": "riskAssessment",
          "name": "RiskAssessment"
        }
      }
    },
    "rootView": {
      "viewName": "ehsm.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}